<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_config"
    tools:context="com.taptester.tappapp.Config_Activity">


    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:background="@drawable/button_background"
        android:layout_alignParentStart="true"
        android:id="@+id/relativeLayout">

        <Button
            android:layout_width="130dp"
            android:layout_height="40dp"
            android:text="@string/one_tap"
            android:id="@+id/tapOneButton"
            android:onClick="openMenu"
            android:background="@drawable/custom_button"
            android:textColor="#FFF"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/not_active"
            android:textColor="#FFF"
            android:id="@+id/textView1"
            android:paddingRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView1"

            android:layout_toStartOf="@+id/textView1"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/textView1"
            android:layout_marginRight="10dp"/>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/relativeLayout"
        android:layout_centerHorizontal="true"
        android:background="@drawable/button_background"
        android:id="@+id/relativeLayout2">


        <Button
            android:layout_width="130dp"
            android:layout_height="40dp"
            android:text="@string/two_tap"
            android:id="@+id/tapTwoButton"
            android:textColor="#FFF"
            android:layout_centerVertical="true"
            android:background="@drawable/custom_button"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/not_active"
            android:id="@+id/textView2"
            android:textColor="#FFF"
            android:paddingRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView2"
            android:layout_toStartOf="@+id/textView2"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/textView2"
            android:layout_marginRight="10dp"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/relativeLayout2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/button_background"
        android:id="@+id/relativeLayout3">
        <Button
            android:layout_width="130dp"
            android:layout_height="40dp"
            android:text="@string/three_tap"
            android:textColor="#FFF"
            android:id="@+id/tapThreeButton"
            android:background="@drawable/custom_button"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/not_active"
            android:id="@+id/textView3"
            android:textColor="#FFF"
            android:paddingRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView3"
            android:layout_toStartOf="@+id/textView3"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/textView3"
            android:layout_marginRight="10dp"/>
        </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/relativeLayout3"
        android:layout_alignParentRight="true"
        android:background="@drawable/button_background"
        android:id="@+id/relativeLayout4"
        android:layout_alignParentEnd="true">
        <Button
            android:layout_width="130dp"
            android:layout_height="40dp"
            android:text="@string/four_tap"
            android:textColor="#FFF"
            android:id="@+id/tapFourButton"
            android:background="@drawable/custom_button"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/not_active"
            android:id="@+id/textView4"
            android:textColor="#FFF"
            android:paddingRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView4"
            android:layout_toStartOf="@+id/textView4"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/textView4"
            android:layout_marginRight="10dp"/>

        </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="@drawable/button_background"
        android:layout_below="@+id/relativeLayout4"
        android:id="@+id/relativeLayout5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <Button
            android:layout_width="130dp"
            android:layout_height="40dp"
            android:text="@string/five_tap"
            android:id="@+id/tapFiveButton"
            android:background="@drawable/custom_button"
            android:textColor="#FFF"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/not_active"
            android:id="@+id/textView5"
            android:textColor="#FFF"
            android:paddingRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView5"
            android:layout_toStartOf="@+id/textView5"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/textView5"
            android:layout_marginRight="10dp"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="@drawable/button_background"
        android:layout_below="@+id/relativeLayout5"
        android:id="@+id/relativeLayout6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <Button
            android:layout_width="130dp"
            android:layout_height="40dp"
            android:text="@string/SlideTXT"
            android:id="@+id/slideTopButton"
            android:drawableRight="@drawable/slide_top"
            android:paddingRight="10dp"
            android:background="@drawable/custom_button"
            android:textColor="#FFF"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/not_active"
            android:id="@+id/textView6"
            android:textColor="#FFF"
            android:paddingRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView6"
            android:layout_toStartOf="@+id/textView6"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/textView6"
            android:layout_marginRight="10dp"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="@drawable/button_background"
        android:layout_below="@+id/relativeLayout6"
        android:id="@+id/relativeLayout7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <Button
            android:layout_width="130dp"
            android:layout_height="40dp"
            android:text="@string/SlideTXT"
            android:id="@+id/slideBottom"
            android:background="@drawable/custom_button"
            android:textColor="#FFF"
            android:drawableRight="@drawable/slide_bottom"
            android:paddingRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/not_active"
            android:id="@+id/textView7"
            android:textColor="#FFF"
            android:paddingRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView7"
            android:layout_toStartOf="@+id/textView7"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/textView7"
            android:layout_marginRight="10dp"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="@drawable/button_background"
        android:layout_below="@+id/relativeLayout7"
        android:id="@+id/relativeLayout8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <Button
            android:layout_width="130dp"
            android:layout_height="40dp"
            android:text="@string/SlideTXT"
            android:id="@+id/slideLeft"
            android:background="@drawable/custom_button"
            android:drawableRight="@drawable/slide_left"
            android:paddingRight="10dp"
            android:textColor="#FFF"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/not_active"
            android:id="@+id/textView8"
            android:textColor="#FFF"
            android:paddingRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView8"
            android:layout_toStartOf="@+id/textView8"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/textView8"
            android:layout_marginRight="10dp"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="@drawable/button_background"
        android:layout_below="@+id/relativeLayout8"
        android:id="@+id/relativeLayout9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <Button
            android:layout_width="130dp"
            android:layout_height="40dp"
            android:text="@string/SlideTXT"
            android:id="@+id/slideRight"
            android:background="@drawable/custom_button"
            android:drawableRight="@drawable/slide_right"
            android:paddingRight="10dp"
            android:textColor="#FFF"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/not_active"
            android:id="@+id/textView9"
            android:textColor="#FFF"
            android:paddingRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView9"
            android:layout_toStartOf="@+id/textView9"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/textView9"
            android:layout_marginRight="10dp"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#000"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/relativeLayout"
        android:layout_alignStart="@+id/relativeLayout"
        android:onClick="smsSettings"
        android:id="@+id/relativeLayout10"
        android:layout_below="@+id/relativeLayout9">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="@string/action_SMS"
            android:textStyle="bold"
            android:textColor="#FFF"
            android:id="@+id/textView"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true" />
    </RelativeLayout>




</RelativeLayout>
